<?xml version="1.0" ?>
<ddtunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ddtunit.sourceforge.net/ddtunit.xsd">
	<cluster id="ActualTest">
		<!-- description>This test cas is used for verifying actual scenarios concerning single test steps.</description -->
		<!--                                                                  -->
		<!-- simple object retrieval                                          -->
		<!--                                                                  -->
		<group id="testMethod">
            <test id="complexUserBean">
                <objs>
                    <obj id="myObj" type="junitx.ddtunit.usecases.UserBean" hint="bean">
                        <id>XXXmyUsername</id>
                        <password>myPassword</password>
                    </obj>
                </objs>
                <asserts>
                    <assert id="result" action="isEqual" type="junitx.ddtunit.usecases.UserBean">
                        <_id>setXXXmyUsername</_id>
                        <password>myPassword</password>
                    </assert>
                </asserts>
            </test>
		</group>
	</cluster>
</ddtunit>
